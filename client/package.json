{
  "name": "backend-interface-dev",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "npm run prettier && npm run lint:check",
    "lint:check": "tslint --project ./tsconfig.json --config ./tslint.json --format codeFrame",
    "lint:ci": "tslint --project ./tsconfig.json --config ./tslint.json --format codeFrame",
    "prettier": "prettier --config './.prettierrc' --write '.huskyrc' '.prettierrc' './**/*.{ts,tsx,js,jsx,json,md,scss,yml}'",
    "prettier:ci": "prettier --config './.prettierrc' --list-different '.huskyrc' '.prettierrc' './**/*.{ts,tsx,js,jsx,json,md,scss,yml}'",
    "coverage": "npm run coverage:clean && npm run coverage:generate",
    "coverage:clean": "rm -rf ./coverage",
    "coverage:generate": "jest --config=jest.json --coverage --passWithNoTests",
    "docs": "npm run docs:clean && npm run docs:generate",
    "docs:clean": "rm -rf ./docs",
    "docs:generate": "typedoc --options './typedoc.json' './src'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@types/express": "^4.16.1",
    "@types/socket.io": "^2.1.2",
    "express": "^4.17.1",
    "install": "^0.12.2",
    "jest": "^24.8.0",
    "moment": "^2.24.0",
    "momentjs": "^2.0.0",
    "ng6-socket-io": "^1.0.3",
    "ngx-moment": "^3.4.0",
    "npm": "^6.13.4",
    "prettier": "^1.17.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "socket.io": "^2.2.0",
    "tslib": "^1.9.0",
    "typedoc": "^0.14.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^2.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.15.0",
    "typescript": "~3.4.3"
  }
}
